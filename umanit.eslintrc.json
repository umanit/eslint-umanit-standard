{
    "extends": "eslint:recommended",
    "rules": {
        "block-scoped-var": 1,
        "brace-style": 2,
        "camelcase": 2,
        "comma-style": 2,
        "computed-property-spacing": 2,
        "consistent-return": 2,
        "dot-location": 2,
        "dot-notation": 2,
        "eol-last": 1,
        "eqeqeq": 2,
        "indent": 2,
        "key-spacing": 2,
        "keyword-spacing": 2,
        "linebreak-style": 1,
        "max-depth": [1,
            6
        ],
        "max-len": [1,
            120
        ],
        "max-nested-callbacks": [1,
            4
        ],
        "max-params": [1,
            5
        ],
        "max-statements-per-line": 1,
        "new-cap": 2,
        "no-array-constructor": 1,
        "no-bitwise": 1,
        "no-cond-assign": 1,
        "no-console": 1,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 1,
        "no-extra-parens": 1,
        "no-floating-decimal": 1,
        "no-implicit-coercion": 2,
        "no-irregular-whitespace": 1,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": 1,
        "no-multiple-empty-lines": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-nested-ternary": 2,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-wrappers": 1,
        "no-return-assign": 1,
        "no-self-compare": 1,
        "no-spaced-func": 2,
        "no-trailing-spaces": 1,
        "no-throw-literal": 2,
        "no-undef": 0,
        "no-unneeded-ternary": 1,
        "no-unused-expressions": 1,
        "no-unused-vars": 0,
        "no-void": 2,
        "no-whitespace-before-property": 2,
        "object-curly-spacing": [2,
            "never"
        ],
        "one-var-declaration-per-line": 2,
        "operator-assignment": [1,
            "always"
        ],
        "operator-linebreak": [2,
            "after"
        ],
        "quote-props": [2,
            "consistent"
        ],
        "quotes": [2,
            "double",
            {
                "avoidEscape": true,
                "allowTemplateLiterals": true
            }
        ],
        "radix": 1,
        "require-jsdoc": [2,
            {
                "require": {
                    "FunctionDeclaration": true,
                    "MethodDefinition": true,
                    "ClassDeclaration": true
                }
            }
        ],
        "semi": [2,
            "always"
        ],
        "semi-spacing": 2,
        "space-before-blocks": 2,
        "space-in-parens": [2,
            "never"
        ],
        "space-infix-ops": 2,
        "space-unary-ops": 2,
        "spaced-comment": [1,
            "always"
        ],
        "strict": [1,
            "global"
        ],
        "valid-jsdoc": 2,
        "wrap-regex": 1
    }
}
